# Coinbase Advanced API credentials
COINBASE_API_KEY=your_api_key_here
COINBASE_API_SECRET=your_api_secret_here

# Trading mode: paper | live
TRADING_MODE=paper

# Paper trading initial balance (USDT)
PAPER_BALANCE=500

# Symbols to trade (comma-separated)
SYMBOLS=BTC/USDT,ETH/USDT

# Strategy interval in minutes (60 = hourly)
INTERVAL_MINUTES=60

# Risk management
STOP_LOSS_PCT=2.0
TAKE_PROFIT_MIN_PCT=1.5
TAKE_PROFIT_MAX_PCT=5.0
MAX_POSITION_PCT=30
MAX_CONCURRENT_POSITIONS=2

# Dashboard password (CHANGE THIS!)
DASHBOARD_PASSWORD=changeme123
